{
  "name": "cordova-plugin-firebase-intercom",
  "version": "0.0.1",
  "description": "Google Firebase Intercom Integration Plugin",
  "scripts": {
    "commitmsg": "commitlint -E GIT_PARAMS --config ./.commitlintrc.yml",
    "clear": "rm -rf scripts",
    "build": "npm run clear && ./node_modules/.bin/tsc --p tsconfig.json"
  },
  "main": "src/scripts/index.ts",
  "cordova": {
    "id": "cordova-plugin-firebase-intercom",
